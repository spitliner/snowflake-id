{
  "name": "snowflake-id",
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "rimraf dist && pkgroll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spitliner/snowflake-id.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spitliner/snowflake-id/issues"
  },
  "homepage": "https://github.com/spitliner/snowflake-id#readme",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "pkgroll": "^2.1.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "xo": "^0.56.0"
  },
  "xo": {
    "space": 4,
    "rules": {
      "capitalized-comments": "off",
      "yoda": "off"
    },
    "ignores": [
      "dist",
      "tests"
    ]
  },
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "tsx": "^4.10.5"
  }
}
